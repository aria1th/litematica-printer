  "authors": [
    "eatmyvenom, jensvh, aria1th"
  ],
  "contact": {
    "homepage": "https://github.com/aria1th/litematica-printer",
    "sources": "https://github.com/aria1th/litematica-printer"
  },

  "license": "CC0-1.0",
